{
    "source": {
        "directories": [
            "src"
        ]
    },
    "phpUnit": {
        "customPath": "tools/phpunit"
    },
    "bootstrap": "vendor/autoload.php",
    "mutators": {
        "@default": true,
        "Continue_": {
            "ignore": [
                "SebastianBergmann\\CsvParser\\Parser::generator"
            ]
        }
    },
    "minMsi": 100,
    "minCoveredMsi": 100,
    "testFrameworkOptions": "--testsuite=unit"
}
